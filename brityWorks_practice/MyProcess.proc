  <JScript><![CDATA[function MyProcess() {

    this.YES24_Browser;
    this.page;
    this.XPath_Titles = "";
    this.XPath_Date = "";
    this.titleElements;
    this.el;
    this.title;
    this.url;
    this.array_Titles = [];
    this.array_URLs = [];
    this.array_Dates = [];
    this.dateElement;
    this.date;
    this.resultText = "";
    this.FunctionAsync;

    this.run = function() {

        //------------------------------------------------
        STEP('S12dc8cb8ff544f39a59f349bca118cc1', '[1] ChromeOpen');
        this.YES24_Browser = StepPlayer.Run('Chrome.ChromeOpen', this.chromeopen_info_1);

        //------------------------------------------------
        STEP('S25bb5ded5c144938ba4d2bb866a140c6', '[2] ExecuteScript');
        this.page = this.YES24_Browser.Page;
        // 페이지 객체로 저장

        this.XPath_Titles ='//*[@id="category_layout"]/tbody/tr/td[3]/p[1]/a[1]';
        this.XPath_Date = '//*[@id="infoset_specific"]/div[2]/div/table/tbody/tr[1]/td';
        // XPath 변수 저장

        //------------------------------------------------
        STEP('Se4ac0757e55944f48f90dc3fbaf04224', '[3] ExecuteScript');
        this.page.WaitForXPathAsync(this.XPath_Titles);
        // 페이지 뜰 때 까지 기다림

        this.titleElements=this.page.XPathAsync(this.XPath_Titles).Result;
        // 제목들 엘리먼트 객체들의 배열로 저장

        //------------------------------------------------
        STEP('Sed9f5412651c4ef08f1d03b6017eb7c6', '[4] Foreach');
        for(this.el of this.titleElements) {

            //------------------------------------------------
            STEP('S50a655e3ba5245ab9d5adc4d7b36bc5f', '[5] ExecuteScript');
            this.title = this.page.EvaluateFunctionAsync("e => e.innerText", this.el).Result;
            // 엘리먼트에서 텍스트 추출

            this.title = this.title.toString();

            this.url = this.page.EvaluateFunctionAsync("e => e.href", this.el).Result;
            // 엘리먼트에서 url 추출

            this.url=this.url.toString();

            if(this.title.includes("데이터")) { 
                this.array_Titles.push(this.title)
                this.array_URLs.push(this.url)
            };
            // 데이터를 포함할 때만 배열에 저장

        //------------------------------------------------
        }
        STEP('S8d463776fd234355aead155aeb37272a', '[6] End-Foreach');

        ////------------------------------------------------
        //STEP('S9e13857b4e9f42edad17b61c901a8715', '[7] ExecuteScript');
        //(async () => {
        //        await Promise.all([
        //        this.page.GoToAsync(this.url),
        //        this.page.WaitForNavigationAsync()
        //    ])

        //        this.dateElement = await this.page.XPathAsync(this.XPath_Date).Result;

        //        this.date = await this.page.EvaluateFunctionAsync("e => e.innerText", this.dateElement[0]);

        //        this.date = await this.date.toString();

        //        await this.array_Dates.push(this.date);
        //})();


        //------------------------------------------------
        STEP('Sbf85e2ff158a459d86cc0f05b9a583a6', '[8] Foreach');
        for(this.url of this.array_URLs) {

            //------------------------------------------------
            STEP('S4d4c3af43d134748b67cd9cdec7f2756', '[9] ExecuteScript');
            (async () => {
                    await Promise.all([
                    this.page.GoToAsync(this.url),
                    this.page.WaitForNavigationAsync()
                ])
            })();
            // 페이지로 이동 + 페이지 로드 기다림

            //------------------------------------------------
            STEP('S9da14a3f46894dd38142ac0464570cc0', '[10] Delay');
            Delay(3000);

            //------------------------------------------------
            STEP('S2a6b0648a8634c83be6c33a2d9371723', '[11] ExecuteScript');
            this.dateElement = this.page.XPathAsync(this.XPath_Date).Result;
            // 날짜 정보 엘리먼트 객체로 저장

            this.date = this.page.EvaluateFunctionAsync("e => e.innerText", this.dateElement[0]).Result;
            // 엘리먼트 객체로부터 텍스트 추출
            this.date = this.date.toString();

            this.array_Dates.push(this.date);
            // 추출한 텍스트 배열에 저장

        //------------------------------------------------
        }
        STEP('S55375d09054c40509b79daa7e8c124a3', '[12] End-Foreach');

        //------------------------------------------------
        STEP('S0a809992c53d4fb4ac56115612961d62', '[13] For');
        for (this.i = 0; this.i < this.array_Titles.length; this.i++) {

            //------------------------------------------------
            STEP('S99af0eab04c94f039f4a90e4c3b6a46e', '[14] ExecuteScript');
            this.resultText = this.resultText+this.array_Titles[this.i].replace(/ /g, "_")+" | "+this.array_URLs[this.i]+" | "+this.array_Dates[this.i].replace(/ /g, "_")+"\n"
            // 문자열 가공

        //------------------------------------------------
        }
        STEP('Sfad3268986064213a6a64132fc815850', '[15] End-For');

        //------------------------------------------------
        STEP('S2adb223c2c3e42708e2f347c43135ffc', '[16] ExecutePythonFile');
        ActivityWorker.Play('Python', 'ExecutePython', this.activitylibraryitem_info_16);


    }

    this.execute = function() {
        var result = null;
        result = this.run()
        if (result != null)
            return result;
    }
}
